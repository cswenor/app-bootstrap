        "eslint": "^9.17.0",
        "eslint-config-next": "^15.1.0",
        "postcss": "^8.4.49",
        "prettier": "^3.4.2",
        "prettier-plugin-tailwindcss": "^0.6.9",
        "prisma": "^6.0.1",
        "tailwindcss": "^3.4.16",
        "typescript": "^5.7.2"
