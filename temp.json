        "@trpc/server": "^11.0.0-rc.446",
        "next": "^15.1.0",
        "react": "^19.0.0",
        "react-dom": "^19.0.0",
        "server-only": "^0.0.1",
        "superjson": "^2.2.2",
        "zod": "^3.24.1"
    },
    "devDependencies": {
        "@types/eslint": "^9.6.1",
